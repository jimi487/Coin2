{
	"api": {
		"coin": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "AR Facial Recoginition",
							"apiKeyExpirationDays": "365"
						}
					}
				}
			}
		},
		"coinAPI": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authcoin04"
						}
					}
				}
			}
		}
	},
	"function": {
		"coin04CustomMessage": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	},
	"auth": {
		"userPoolGroups": {
			"service": "Cognito-UserPool-Groups",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "coin04",
					"attributes": [
						"UserPoolId",
						"AppClientIDWeb",
						"AppClientID",
						"IdentityPoolId"
					]
				}
			]
		},
		"coin04": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "coin04CustomMessage",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				}
			]
		}
	},
	"storage": {
		"CoinBank": {
			"service": "S3",
			"providerPlugin": "awscloudformation"
		}
	}
}